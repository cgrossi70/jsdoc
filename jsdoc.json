{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
      "include": [ "src" ],
      "exclude": [ "node_modules", "docs" ],
      "includePattern": ".js$"
  },
  "sourceType": "module",
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash",
    "tutorials": "./tutorials",
    "readme": "./README.md"
  }
}