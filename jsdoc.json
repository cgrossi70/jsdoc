{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
      "include": [ "src" ],
      "exclude": [ "node_modules" ],
      "includePattern": ".js$"
  },

  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash"
  }
}